<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/gray_back_2"
    android:orientation="vertical">


    <de.blinkt.openvpn.views.TitleBar
        android:id="@+id/title"
        android:layout_width="match_parent"
        android:layout_height="@dimen/padding_50dp"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"/>


    <ScrollView
        android:id="@+id/accountScrollView"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true"
        android:layout_marginTop="@dimen/padding_50dp"
        android:overScrollMode="never"
        android:scrollbars="none">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">

            <ImageView
                android:id="@+id/blueImageView"
                android:layout_width="match_parent"
                android:layout_height="100dp"
                android:layout_alignParentStart="true"
                android:background="@color/blue_title"/>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="50dp"

                android:orientation="vertical"
                android:paddingLeft="@dimen/padding_15dp"
                android:paddingRight="@dimen/padding_15dp">

                <RelativeLayout
                    android:id="@+id/rl_people_center"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white">


                    <TextView
                        android:id="@+id/accountNameTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="@dimen/padding_40dp"
                        android:text="@string/unset_name"
                        android:textSize="@dimen/little_14"/>

                    <TextView
                        android:id="@+id/accountPhoneTextView"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@id/accountNameTextView"
                        android:layout_centerHorizontal="true"
                        android:layout_marginBottom="@dimen/padding_15dp"
                        android:text="@string/no_data_show"
                        android:textColor="@color/gray_text"
                        android:textSize="@dimen/little_14"/>


                </RelativeLayout>

                <View
                    style="@style/horizontal_line_view"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:gravity="center"
                    android:orientation="vertical">

                    <RelativeLayout
                        android:id="@+id/balanceRelativeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/call_phone_selector"
                        android:paddingBottom="@dimen/padding_10dp"
                        android:paddingLeft="@dimen/padding_15dp"
                        android:paddingRight="@dimen/padding_15dp"
                        android:paddingTop="@dimen/padding_10dp">


                        <ImageView
                            android:id="@+id/balanceImageView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="12dp"
                            android:background="@drawable/balance_icon"
                            />

                        <TextView
                            android:id="@+id/balanceTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/balanceImageView"
                            android:text="@string/balance"
                            android:textSize="15sp"/>

                        <TextView
                            android:id="@+id/rechargeTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:background="@drawable/recharge_btn_selector"
                            android:gravity="center"
                            android:text="@string/recharge"
                            android:textColor="@drawable/blue_btn_click_color_selecor"
                            android:textSize="@dimen/little_12"
                            />

                    </RelativeLayout>

                    <View
                        style="@style/horizontal_line_view"
                        />

                    <RelativeLayout
                        android:id="@+id/billRelativeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_weight="1"
                        android:background="@drawable/call_phone_selector"
                        android:paddingBottom="@dimen/padding_10dp"
                        android:paddingLeft="@dimen/padding_15dp"
                        android:paddingRight="@dimen/padding_15dp"
                        android:paddingTop="@dimen/padding_10dp">


                        <ImageView
                            android:id="@+id/billImageView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_marginRight="12dp"
                            android:src="@drawable/bill_icon"
                            />

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerVertical="true"
                            android:layout_toRightOf="@id/billImageView"
                            android:text="@string/bill"
                            android:textSize="15sp"/>
                    </RelativeLayout>
                </LinearLayout>

                <View
                    style="@style/horizontal_line_view"
                    />

                <View
                    style="@style/horizontal_line_view"
                    android:layout_marginTop="@dimen/padding_5dp"
                    />

                <RelativeLayout
                    android:id="@+id/activateRelativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    >

                    <RelativeLayout
                        android:id="@+id/noPacketRelativeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:background="@color/white">

                        <TextView
                            android:id="@+id/add_or_activate_package"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:drawableLeft="@drawable/activate_device_account"
                            android:drawablePadding="@dimen/padding_10dp"
                            android:text="@string/activate_packet"
                            android:textColor="@color/gray_text"/>

                    </RelativeLayout>
                    <RelativeLayout
                        android:id="@+id/PacketRelativeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/padding_5dp"
                        android:background="@drawable/has_package"
                        android:visibility="visible"
                        >

                        <TextView

                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="12dp"
                            android:text="@string/package_surplus_detail"
                            android:textColor="@color/white"
                            android:textSize="16sp"
                            />

                        <TextView
                            android:id="@+id/going_buy"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:paddingBottom="12dp"
                            android:paddingRight="12dp"
                            android:paddingTop="12dp"
                            android:text="@string/going_buy"
                            android:textColor="@color/white"
                            />

                        <View
                            android:id="@+id/line1"
                            style="@style/horizontal_line_view"
                            android:layout_below="@id/going_buy"
                            />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/line1"
                            >

                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">

                                <TextView
                                    android:id="@+id/call"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="15dp"
                                    android:layout_centerHorizontal="true"
                                    android:text="@string/call_time"
                                    android:textColor="@color/white"

                                    />

                                <TextView
                                    android:id="@+id/call_time"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/call"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginLeft="@dimen/padding_15dp"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/white"
                                    android:textSize="18sp"
                                    android:textStyle="bold"
                                    />
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="0dp"

                                android:layout_height="wrap_content"
                                android:layout_weight="1">

                                <TextView
                                    android:id="@+id/flow"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/padding_15dp"
                                    android:layout_marginTop="15dp"
                                    android:layout_centerHorizontal="true"
                                    android:text="@string/flow_count"
                                    android:textColor="@color/white"
                                    />

                                <TextView
                                    android:id="@+id/flow_count"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/flow"
                                    android:layout_marginLeft="@dimen/padding_15dp"
                                    android:layout_marginTop="5dp"
                                    android:layout_centerHorizontal="true"
                                    android:textColor="@color/white"
                                    android:textSize="18sp"
                                    android:textStyle="bold"
                                    />
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="0dp"
                                android:gravity="center"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">

                                <TextView
                                    android:id="@+id/all_package"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/padding_15dp"
                                    android:layout_marginTop="15dp"
                                    android:text="@string/all_package_count"
                                    android:layout_centerHorizontal="true"
                                    android:textColor="@color/white"
                                    />

                                <TextView
                                    android:id="@+id/package_all_count"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/all_package"
                                    android:layout_marginLeft="@dimen/padding_15dp"
                                    android:layout_centerHorizontal="true"
                                    android:layout_marginTop="5dp"
                                    android:textColor="@color/white"
                                    android:textSize="18sp"
                                    android:textStyle="bold"
                                    />
                            </RelativeLayout>
                        </LinearLayout>
                    </RelativeLayout>

                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/addDeviceRelativeLayout"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    >


                    <RelativeLayout
                        android:id="@+id/noDeviceRelativeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_alignParentStart="true"
                        android:layout_marginTop="@dimen/padding_5dp"
                        android:background="@color/white"
                        >

                        <TextView
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_centerHorizontal="true"
                            android:layout_centerVertical="true"
                            android:drawableLeft="@drawable/add_device"
                            android:drawablePadding="@dimen/padding_10dp"
                            android:text="@string/add_device"
                            android:textColor="@color/gray_text"/>


                    </RelativeLayout>

                    <RelativeLayout
                        android:id="@+id/deviceSummarizedRelativeLayout"
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:layout_marginTop="@dimen/padding_5dp"
                        android:background="@drawable/has_device"
                        android:visibility="gone"
                        >

                        <TextView
                            android:id="@+id/deviceNameTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:padding="12dp"
                            android:text="@string/unibox_key"
                            android:textColor="@color/white"
                            android:textSize="16sp"
                            />

                        <TextView
                            android:id="@+id/unBindTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:layout_alignParentRight="true"
                            android:paddingBottom="12dp"
                            android:paddingRight="12dp"
                            android:paddingTop="12dp"
                            android:text="@string/remove_bind_under_line"
                            android:textColor="@color/white"
                            />

                        <View
                            android:id="@+id/addDeviceLine"
                            style="@style/horizontal_line_view"
                            android:layout_below="@id/deviceNameTextView"
                            />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:layout_below="@id/addDeviceLine"
                            >

                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">

                                <TextView
                                    android:id="@+id/powerTitleTextView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginTop="15dp"
                                    android:paddingLeft="@dimen/padding_15dp"
                                    android:text="@string/power"
                                    android:textColor="@color/white"
                                    />

                                <TextView
                                    android:id="@+id/powerTextView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/powerTitleTextView"
                                    android:layout_marginLeft="@dimen/padding_15dp"
                                    android:layout_marginTop="5dp"
                                    android:text="----"
                                    android:textColor="@color/white"
                                    android:textSize="18sp"
                                    android:textStyle="bold"
                                    />
                            </RelativeLayout>

                            <RelativeLayout
                                android:layout_width="0dp"
                                android:layout_height="wrap_content"
                                android:layout_weight="1">

                                <ImageView
                                    android:id="@+id/signalIconImageView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_marginLeft="@dimen/padding_15dp"
                                    android:layout_marginTop="20dp"
                                    android:background="@drawable/unregist"
                                    />

                                <TextView
                                    android:id="@+id/operatorTextView"
                                    android:layout_width="wrap_content"
                                    android:layout_height="wrap_content"
                                    android:layout_below="@id/signalIconImageView"
                                    android:layout_marginLeft="@dimen/padding_15dp"
                                    android:layout_marginTop="5dp"
                                    android:text="----"
                                    android:textColor="@color/white"
                                    android:textSize="18sp"
                                    android:textStyle="bold"
                                    />
                            </RelativeLayout>


                        </LinearLayout>
                    </RelativeLayout>
                </RelativeLayout>

                <View
                    style="@style/horizontal_line_view"
                    android:layout_marginTop="5dp"
                    />

                <View
                    style="@style/horizontal_line_view"
                    android:layout_marginTop="@dimen/padding_5dp"
                    />

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical">
                    <!--
                                    <TextView
                                        android:id="@+id/tv_my_pakages"
                                        android:layout_width="match_parent"
                                        android:layout_height="@dimen/padding_40dp"
                                        android:background="@drawable/call_phone_selector"
                                        android:drawableLeft="@drawable/my_package"
                                        android:drawablePadding="@dimen/padding"
                                        android:drawableRight="@drawable/arrow"
                                        android:gravity="center_vertical"
                                        android:paddingLeft="@dimen/padding_15dp"
                                        android:paddingRight="@dimen/padding_15dp"
                                        android:text="@string/my_pakages"
                                        android:textSize="14sp"/>-->

                    <!--<TextView
                        android:id="@+id/tv_my_device"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/padding_40dp"
                        android:background="@drawable/call_phone_selector"
                        android:drawableLeft="@drawable/my_device"
                        android:drawablePadding="@dimen/padding"
                        android:drawableRight="@drawable/arrow"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/padding_15dp"
                        android:paddingRight="@dimen/padding_15dp"
                        android:text="@string/my_device"
                        android:textSize="14sp"/>-->


                    <TextView
                        android:id="@+id/permission_set"
                        android:layout_width="match_parent"
                        android:layout_height="@dimen/padding_40dp"
                        android:background="@drawable/call_phone_selector"
                        android:drawableLeft="@drawable/permission_image"
                        android:drawablePadding="@dimen/padding"
                        android:drawableRight="@drawable/arrow"
                        android:gravity="center_vertical"
                        android:paddingLeft="@dimen/padding_15dp"
                        android:paddingRight="@dimen/padding_15dp"
                        android:text="@string/permission_set"
                        android:textSize="14sp"/>
                </LinearLayout>

                <View
                    style="@style/horizontal_line_view"
                    />

                <!--<View
                    style="@style/horizontal_line_view"
                    android:layout_marginTop="15dp"
                    />-->
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:background="@color/white"
                    android:orientation="vertical"
                    android:visibility="gone">

                    <LinearLayout
                        android:id="@+id/ll_coming_tel_tip"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/call_phone_selector"
                        android:gravity="center_vertical"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/padding_15dp"
                        android:paddingRight="@dimen/padding_15dp"
                        >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="@dimen/padding_40dp"
                            android:layout_weight="1"
                            android:drawableLeft="@drawable/coming_tel_tip"
                            android:drawablePadding="@dimen/padding"
                            android:gravity="center_vertical"
                            android:text="@string/coming_tel_tip"
                            android:textSize="14sp"/>

                        <TextView
                            android:id="@+id/tv_coming_tel_tip"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/padding_40dp"
                            android:drawablePadding="@dimen/padding"
                            android:drawableRight="@drawable/arrow"
                            android:gravity="center_vertical"
                            android:text="@string/unopened"
                            android:textColor="@color/gray_text"
                            android:textSize="14sp"/>
                    </LinearLayout>

                    <View
                        style="@style/horizontal_line_view_margin_15dp"

                        />

                    <LinearLayout
                        android:id="@+id/ll_alarm_clock_tip"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/call_phone_selector"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/padding_15dp"
                        android:paddingRight="@dimen/padding_15dp"
                        >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="@dimen/padding_40dp"
                            android:layout_weight="1"
                            android:drawableLeft="@drawable/alarm_clock"
                            android:drawablePadding="@dimen/padding"
                            android:gravity="center_vertical"
                            android:text="@string/alarm_clock_tip"
                            android:textSize="14sp"/>

                        <TextView
                            android:id="@+id/tv_alarm_clock_tip"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/padding_40dp"
                            android:drawablePadding="@dimen/padding"
                            android:drawableRight="@drawable/arrow"
                            android:gravity="center_vertical"
                            android:text="@string/opened_num_alarm_clock"
                            android:textColor="@color/gray_text"
                            android:textSize="14sp"/>
                    </LinearLayout>

                    <View
                        style="@style/horizontal_line_view_margin_15dp"
                        />

                    <LinearLayout
                        android:id="@+id/ll_sms_tip"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/call_phone_selector"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/padding_15dp"
                        android:paddingRight="@dimen/padding_15dp"
                        >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="@dimen/padding_40dp"
                            android:layout_weight="1"
                            android:drawableLeft="@drawable/sms_tip"
                            android:drawablePadding="@dimen/padding"
                            android:gravity="center_vertical"
                            android:text="@string/sms_tip"
                            android:textSize="14sp"/>

                        <TextView
                            android:id="@+id/tv_sms_tip"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/padding_40dp"
                            android:drawablePadding="@dimen/padding"
                            android:drawableRight="@drawable/arrow"
                            android:gravity="center_vertical"
                            android:text="@string/unopened"
                            android:textColor="@color/gray_text"
                            android:textSize="14sp"/>
                    </LinearLayout>

                    <View

                        style="@style/horizontal_line_view_margin_15dp"
                        />

                    <LinearLayout
                        android:id="@+id/ll_weixin_tip"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/call_phone_selector"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/padding_15dp"
                        android:paddingRight="@dimen/padding_15dp"
                        >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="@dimen/padding_40dp"
                            android:layout_weight="1"
                            android:drawableLeft="@drawable/weixin_meg"
                            android:drawablePadding="@dimen/padding"
                            android:gravity="center_vertical"
                            android:text="@string/weixin_tip"
                            android:textSize="14sp"/>

                        <TextView
                            android:id="@+id/tv_weixin_tip"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/padding_40dp"
                            android:drawablePadding="@dimen/padding"
                            android:drawableRight="@drawable/arrow"
                            android:gravity="center_vertical"
                            android:text="@string/unopened"
                            android:textColor="@color/gray_text"
                            android:textSize="14sp"/>
                    </LinearLayout>

                    <View

                        style="@style/horizontal_line_view_margin_15dp"
                        />

                    <LinearLayout
                        android:id="@+id/ll_qq_tip"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/call_phone_selector"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/padding_15dp"
                        android:paddingRight="@dimen/padding_15dp"
                        >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="@dimen/padding_40dp"
                            android:layout_weight="1"
                            android:drawableLeft="@drawable/qq_mes"
                            android:drawablePadding="@dimen/padding"
                            android:gravity="center_vertical"
                            android:text="@string/qq_tip"
                            android:textSize="14sp"/>

                        <TextView
                            android:id="@+id/tv_qq_tip"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/padding_40dp"
                            android:drawablePadding="@dimen/padding"
                            android:drawableRight="@drawable/arrow"
                            android:gravity="center_vertical"
                            android:text="@string/unopened"
                            android:textColor="@color/gray_text"
                            android:textSize="14sp"/>
                    </LinearLayout>

                    <View
                        style="@style/horizontal_line_view_margin_15dp"
                        />

                    <LinearLayout
                        android:id="@+id/liftWristLinearLayout"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:background="@drawable/call_phone_selector"
                        android:orientation="horizontal"
                        android:paddingLeft="@dimen/padding_15dp"
                        android:paddingRight="@dimen/padding_15dp"
                        >

                        <TextView
                            android:layout_width="0dp"
                            android:layout_height="@dimen/padding_40dp"
                            android:layout_weight="1"

                            android:drawableLeft="@drawable/coming_tel_tip"
                            android:drawablePadding="@dimen/padding"
                            android:gravity="center_vertical"
                            android:text="@string/lift_wrist"
                            android:textSize="14sp"/>

                        <TextView
                            android:id="@+id/liftWristTextView"
                            android:layout_width="wrap_content"
                            android:layout_height="@dimen/padding_40dp"
                            android:drawablePadding="@dimen/padding"
                            android:drawableRight="@drawable/arrow"
                            android:gravity="center_vertical"
                            android:text="@string/unopened"
                            android:textColor="@color/gray_text"
                            android:textSize="14sp"/>
                    </LinearLayout>
                </LinearLayout>

                <View
                    style="@style/horizontal_line_view"
                    />
                <!-- <View
                     style="@style/horizontal_line_view"
                     android:layout_marginTop="15dp"
                     />-->

                <TextView
                    android:id="@+id/tv_setting"
                    android:layout_width="match_parent"
                    android:layout_height="@dimen/padding_40dp"
                    android:background="@drawable/call_phone_selector"
                    android:drawableLeft="@drawable/setting_icon"
                    android:drawablePadding="@dimen/padding"
                    android:drawableRight="@drawable/arrow"
                    android:gravity="center_vertical"
                    android:paddingLeft="@dimen/padding_15dp"
                    android:paddingRight="@dimen/padding_15dp"
                    android:text="@string/setting"
                    android:textSize="14sp"/>

                <View
                    style="@style/horizontal_line_view"
                    />

                <View
                    style="@style/horizontal_line_view"
                    android:layout_marginTop="15dp"
                    />
            </LinearLayout>


            <ImageView
                android:id="@+id/headImageView"
                android:layout_width="70dp"
                android:layout_height="70dp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="20dp"
                android:background="@drawable/default_head"/>
        </RelativeLayout>
    </ScrollView>
